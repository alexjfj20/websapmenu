{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref, computed, watch, onMounted, onUnmounted } from 'vue';\nimport { useRoute, useRouter } from 'vue-router';\nimport DialogContainer from './components/DialogContainer.vue';\nimport { webSocketState, initializeWebSocket, reconnect, closeWebSocket } from './services/webSocketService';\nexport default {\n  name: 'App',\n  components: {\n    DialogContainer\n  },\n  setup() {\n    const route = useRoute();\n    const router = useRouter();\n    const isLoggedIn = ref(localStorage.getItem('isLoggedIn') === 'true');\n\n    // WebSocket state\n    const wsState = webSocketState;\n\n    // No mostrar navegación en la página de menú compartido\n    const showNav = computed(() => {\n      return !route.path.match(/^\\/menu\\/[^\\/]+$/);\n    });\n\n    // Verificar estado de autenticación en cambios de ruta\n    watch(() => route.path, () => {\n      isLoggedIn.value = localStorage.getItem('isLoggedIn') === 'true';\n    });\n\n    // Función de logout\n    const logout = () => {\n      localStorage.removeItem('isLoggedIn');\n      isLoggedIn.value = false;\n      router.push('/');\n    };\n\n    // Inicializar WebSocket al montar el componente\n    onMounted(() => {\n      // Solo intentar conectar si hay una URL configurada - en lugar de la URL ngrok, usamos una URL relativa\n      // que funcionará en el mismo servidor donde está alojada la aplicación\n      try {\n        initializeWebSocket(`${window.location.protocol === 'https:' ? 'wss' : 'ws'}://${window.location.host}/ws`, {\n          reconnectInterval: 2000,\n          reconnectAttempts: 10\n        });\n      } catch (error) {\n        console.error('Error al inicializar WebSocket:', error);\n      }\n    });\n\n    // Limpiar WebSocket al desmontar el componente\n    onUnmounted(() => {\n      closeWebSocket();\n    });\n\n    // Manejar reconexión manual\n    const handleReconnect = () => {\n      reconnect();\n    };\n    return {\n      isLoggedIn,\n      showNav,\n      logout,\n      wsState,\n      handleReconnect\n    };\n  }\n};","map":{"version":3,"names":["ref","computed","watch","onMounted","onUnmounted","useRoute","useRouter","DialogContainer","webSocketState","initializeWebSocket","reconnect","closeWebSocket","name","components","setup","route","router","isLoggedIn","localStorage","getItem","wsState","showNav","path","match","value","logout","removeItem","push","window","location","protocol","host","reconnectInterval","reconnectAttempts","error","console","handleReconnect"],"sources":["F:\\Driver google\\VUE.JS-2\\temp\\websap\\src\\App.vue"],"sourcesContent":["<template>\r\n  <div id=\"app\">\r\n    <!-- WebSocket Status Indicator -->\r\n    <div v-if=\"wsState.reconnecting\" class=\"ws-status reconnecting\">\r\n      <span class=\"loading-spinner\"></span> Reconectando...\r\n    </div>\r\n    <div v-else-if=\"wsState.error\" class=\"ws-status error\">\r\n      <span class=\"error-icon\">⚠️</span> Error de conexión: {{ wsState.error }}\r\n      <button @click=\"handleReconnect\" class=\"reconnect-btn\">Reconectar</button>\r\n    </div>\r\n    \r\n    <nav class=\"main-nav\" v-if=\"showNav\">\r\n      <div class=\"nav-container\">\r\n        <router-link to=\"/\" class=\"nav-logo\">WebSAP</router-link>\r\n        <div class=\"nav-links\">\r\n          <router-link to=\"/\">Inicio</router-link>\r\n          <router-link to=\"/about\">Acerca de</router-link>\r\n          <template v-if=\"isLoggedIn\">\r\n            <router-link to=\"/menu\">Menú</router-link>\r\n            <a href=\"#\" @click.prevent=\"logout\">Cerrar Sesión</a>\r\n          </template>\r\n          <template v-else>\r\n            <router-link to=\"/login\">Iniciar Sesión</router-link>\r\n            <router-link to=\"/register\">Registrarse</router-link>\r\n          </template>\r\n        </div>\r\n      </div>\r\n    </nav>\r\n    \r\n    <router-view/>\r\n    \r\n    <!-- Dialog component for global usage -->\r\n    <DialogContainer />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, computed, watch, onMounted, onUnmounted } from 'vue';\r\nimport { useRoute, useRouter } from 'vue-router';\r\nimport DialogContainer from './components/DialogContainer.vue';\r\nimport { webSocketState, initializeWebSocket, reconnect, closeWebSocket } from './services/webSocketService';\r\n\r\nexport default {\r\n  name: 'App',\r\n  components: {\r\n    DialogContainer\r\n  },\r\n  setup() {\r\n    const route = useRoute();\r\n    const router = useRouter();\r\n    const isLoggedIn = ref(localStorage.getItem('isLoggedIn') === 'true');\r\n    \r\n    // WebSocket state\r\n    const wsState = webSocketState;\r\n    \r\n    // No mostrar navegación en la página de menú compartido\r\n    const showNav = computed(() => {\r\n      return !route.path.match(/^\\/menu\\/[^\\/]+$/);\r\n    });\r\n    \r\n    // Verificar estado de autenticación en cambios de ruta\r\n    watch(() => route.path, () => {\r\n      isLoggedIn.value = localStorage.getItem('isLoggedIn') === 'true';\r\n    });\r\n    \r\n    // Función de logout\r\n    const logout = () => {\r\n      localStorage.removeItem('isLoggedIn');\r\n      isLoggedIn.value = false;\r\n      router.push('/');\r\n    };\r\n    \r\n    // Inicializar WebSocket al montar el componente\r\n    onMounted(() => {\r\n      // Solo intentar conectar si hay una URL configurada - en lugar de la URL ngrok, usamos una URL relativa\r\n      // que funcionará en el mismo servidor donde está alojada la aplicación\r\n      try {\r\n        initializeWebSocket(`${window.location.protocol === 'https:' ? 'wss' : 'ws'}://${window.location.host}/ws`, {\r\n          reconnectInterval: 2000,\r\n          reconnectAttempts: 10\r\n        });\r\n      } catch (error) {\r\n        console.error('Error al inicializar WebSocket:', error);\r\n      }\r\n    });\r\n    \r\n    // Limpiar WebSocket al desmontar el componente\r\n    onUnmounted(() => {\r\n      closeWebSocket();\r\n    });\r\n    \r\n    // Manejar reconexión manual\r\n    const handleReconnect = () => {\r\n      reconnect();\r\n    };\r\n    \r\n    return {\r\n      isLoggedIn,\r\n      showNav,\r\n      logout,\r\n      wsState,\r\n      handleReconnect\r\n    };\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n/* Estilos globales */\r\n* {\r\n  box-sizing: border-box;\r\n  margin: 0;\r\n  padding: 0;\r\n}\r\n\r\nbody {\r\n  font-family: 'Avenir', Helvetica, Arial, sans-serif;\r\n  -webkit-font-smoothing: antialiased;\r\n  -moz-osx-font-smoothing: grayscale;\r\n  color: #2c3e50;\r\n  background-color: #f5f5f5;\r\n}\r\n\r\n#app {\r\n  min-height: 100vh;\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n/* Estilos para la navegación */\r\n.main-nav {\r\n  background-color: #ffffff;\r\n  box-shadow: 0 2px 8px rgba(0,0,0,0.1);\r\n  padding: 15px 0;\r\n}\r\n\r\n.nav-container {\r\n  max-width: 1200px;\r\n  margin: 0 auto;\r\n  padding: 0 20px;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n\r\n.nav-logo {\r\n  font-size: 24px;\r\n  font-weight: bold;\r\n  color: #4CAF50;\r\n  text-decoration: none;\r\n}\r\n\r\n.nav-links {\r\n  display: flex;\r\n  gap: 20px;\r\n}\r\n\r\n.nav-links a {\r\n  color: #555;\r\n  text-decoration: none;\r\n  padding: 5px 10px;\r\n  border-radius: 4px;\r\n  transition: all 0.3s;\r\n}\r\n\r\n.nav-links a:hover {\r\n  background-color: rgba(76, 175, 80, 0.1);\r\n  color: #4CAF50;\r\n}\r\n\r\n.nav-links a.router-link-exact-active {\r\n  color: #4CAF50;\r\n  font-weight: bold;\r\n}\r\n\r\n/* Ajustes responsivos */\r\n@media (max-width: 768px) {\r\n  .nav-container {\r\n    flex-direction: column;\r\n    gap: 15px;\r\n  }\r\n  \r\n  .nav-links {\r\n    flex-wrap: wrap;\r\n    justify-content: center;\r\n  }\r\n}\r\n\r\n/* WebSocket status styles */\r\n.ws-status {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  right: 0;\r\n  padding: 6px 15px;\r\n  font-size: 12px;\r\n  color: white;\r\n  z-index: 2000;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  gap: 8px;\r\n}\r\n\r\n.ws-status.reconnecting {\r\n  background-color: #ff9800;\r\n}\r\n\r\n.ws-status.error {\r\n  background-color: #f44336;\r\n}\r\n\r\n.reconnect-btn {\r\n  background-color: white;\r\n  color: #f44336;\r\n  border: none;\r\n  border-radius: 4px;\r\n  padding: 2px 8px;\r\n  font-size: 11px;\r\n  cursor: pointer;\r\n  margin-left: 8px;\r\n}\r\n\r\n.reconnect-btn:hover {\r\n  background-color: #f9f9f9;\r\n}\r\n\r\n.loading-spinner {\r\n  display: inline-block;\r\n  width: 12px;\r\n  height: 12px;\r\n  border: 2px solid rgba(255, 255, 255, 0.3);\r\n  border-radius: 50%;\r\n  border-top-color: white;\r\n  animation: spin 1s linear infinite;\r\n}\r\n\r\n.error-icon {\r\n  font-size: 14px;\r\n}\r\n\r\n@keyframes spin {\r\n  to {\r\n    transform: rotate(360deg);\r\n  }\r\n}\r\n</style>"],"mappings":";AAqCA,SAASA,GAAG,EAAEC,QAAQ,EAAEC,KAAK,EAAEC,SAAS,EAAEC,WAAU,QAAS,KAAK;AAClE,SAASC,QAAQ,EAAEC,SAAQ,QAAS,YAAY;AAChD,OAAOC,eAAc,MAAO,kCAAkC;AAC9D,SAASC,cAAc,EAAEC,mBAAmB,EAAEC,SAAS,EAAEC,cAAa,QAAS,6BAA6B;AAE5G,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVN;EACF,CAAC;EACDO,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIV,QAAQ,CAAC,CAAC;IACxB,MAAMW,MAAK,GAAIV,SAAS,CAAC,CAAC;IAC1B,MAAMW,UAAS,GAAIjB,GAAG,CAACkB,YAAY,CAACC,OAAO,CAAC,YAAY,MAAM,MAAM,CAAC;;IAErE;IACA,MAAMC,OAAM,GAAIZ,cAAc;;IAE9B;IACA,MAAMa,OAAM,GAAIpB,QAAQ,CAAC,MAAM;MAC7B,OAAO,CAACc,KAAK,CAACO,IAAI,CAACC,KAAK,CAAC,kBAAkB,CAAC;IAC9C,CAAC,CAAC;;IAEF;IACArB,KAAK,CAAC,MAAMa,KAAK,CAACO,IAAI,EAAE,MAAM;MAC5BL,UAAU,CAACO,KAAI,GAAIN,YAAY,CAACC,OAAO,CAAC,YAAY,MAAM,MAAM;IAClE,CAAC,CAAC;;IAEF;IACA,MAAMM,MAAK,GAAIA,CAAA,KAAM;MACnBP,YAAY,CAACQ,UAAU,CAAC,YAAY,CAAC;MACrCT,UAAU,CAACO,KAAI,GAAI,KAAK;MACxBR,MAAM,CAACW,IAAI,CAAC,GAAG,CAAC;IAClB,CAAC;;IAED;IACAxB,SAAS,CAAC,MAAM;MACd;MACA;MACA,IAAI;QACFM,mBAAmB,CAAC,GAAGmB,MAAM,CAACC,QAAQ,CAACC,QAAO,KAAM,QAAO,GAAI,KAAI,GAAI,IAAI,MAAMF,MAAM,CAACC,QAAQ,CAACE,IAAI,KAAK,EAAE;UAC1GC,iBAAiB,EAAE,IAAI;UA