b.id === tabId)) {\n        activeTab.value = tabId;\n      }\n    };\n\n    // Cerrar sesi贸n\n    const handleLogout = async () => {\n      try {\n        logout();\n        router.push('/login');\n      } catch (error) {\n        console.error('Error al cerrar sesi贸n:', error);\n        await alert('Error al cerrar sesi贸n. Por favor, intente nuevamente.');\n      }\n    };\n    return {\n      activeTab,\n      currentUser,\n      tabs,\n      changeTab,\n      logout: handleLogout\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","useRouter","getCurrentUser","logout","hasRole","alert","AdminDashboard","AdminInventory","MenuComponent","name","components","setup","router","activeTab","currentUser","tabs","id","icon","user","title","push","value","error","console","changeTab","tabId","some","tab","handleLogout"],"sources":["F:\\Driver google\\VUE.JS-2\\temp\\websap\\src\\views\\AdminView.vue"],"sourcesContent":["<template>\r\n  <div class=\"admin-container\">\r\n    <header class=\"admin-header\">\r\n      <h1>Panel de Administraci贸n</h1>\r\n      <div class=\"user-info\">\r\n        <span>{{ currentUser?.nombre || 'Administrador' }}</span>\r\n        <button @click=\"logout\" class=\"logout-btn\">Cerrar Sesi贸n</button>\r\n      </div>\r\n    </header>\r\n\r\n    <div class=\"admin-layout\">\r\n      <!-- Barra lateral de navegaci贸n -->\r\n      <aside class=\"sidebar\">\r\n        <nav class=\"nav-menu\">\r\n          <button \r\n            v-for=\"tab in tabs\" \r\n            :key=\"tab.id\"\r\n            :class=\"['nav-item', { active: activeTab === tab.id }]\"\r\n            @click=\"activeTab = tab.id\"\r\n          >\r\n            <span class=\"nav-icon\">{{ tab.icon }}</span>\r\n            <span class=\"nav-text\">{{ tab.name }}</span>\r\n          </button>\r\n        </nav>\r\n      </aside>\r\n\r\n      <!-- Contenido principal -->\r\n      <main class=\"content\">\r\n        <!-- Dashboard -->\r\n        <div v-if=\"activeTab === 'dashboard'\" class=\"tab-content\">\r\n          <AdminDashboard @change-tab=\"changeTab\" />\r\n        </div>\r\n\r\n        <!-- Inventario -->\r\n        <div v-if=\"activeTab === 'inventory'\" class=\"tab-content\">\r\n          <AdminInventory />\r\n        </div>\r\n\r\n        <!-- Gesti贸n de productos -->\r\n        <div v-if=\"activeTab === 'products'\" class=\"tab-content\">\r\n          <MenuComponent />\r\n        </div>\r\n\r\n        <!-- Reportes -->\r\n        <div v-if=\"activeTab === 'reports'\" class=\"tab-content\">\r\n          <div class=\"coming-soon\">\r\n            <h2>Reportes</h2>\r\n            <p>El m贸dulo de reportes estar谩 disponible pr贸ximamente.</p>\r\n          </div>\r\n        </div>\r\n      </main>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, onMounted } from 'vue';\r\nimport { useRouter } from 'vue-router';\r\nimport { getCurrentUser, logout, hasRole } from '../services/authService';\r\nimport { alert } from '../services/dialogService';\r\nimport AdminDashboard from '../components/admin/AdminDashboard.vue';\r\nimport AdminInventory from '../components/admin/AdminInventory.vue';\r\nimport MenuComponent from '../components/MenuComponent.vue';\r\n\r\nexport default {\r\n  name: 'AdminView',\r\n  components: {\r\n    AdminDashboard,\r\n    AdminInventory,\r\n    MenuComponent\r\n  },\r\n  setup() {\r\n    const router = useRouter();\r\n    const activeTab = ref('dashboard');\r\n    const currentUser = ref(null);\r\n    \r\n    // Definir pesta帽as disponibles\r\n    const tabs = [\r\n      { id: 'dashboard', name: 'Dashboard', icon: '' },\r\n      { id: 'inventory', name: 'Inventario', icon: '' },\r\n      { id: 'products', name: 'Gesti贸n de Productos', icon: '' },\r\n      { id: 'reports', name: 'Reportes', icon: '' }\r\n    ];\r\n\r\n    onMounted(async () => {\r\n      try {\r\n        // Verificar permisos de Administrador\r\n        const user = getCurrentUser();\r\n        \r\n        if (!user) {\r\n