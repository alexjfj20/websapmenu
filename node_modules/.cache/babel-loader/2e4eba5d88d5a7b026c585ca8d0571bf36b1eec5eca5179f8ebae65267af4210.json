{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.for-each.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport \"core-js/modules/es.iterator.reduce.js\";\nimport { getMenu } from '../services/menuService';\nexport default {\n  name: 'SharedMenuView',\n  data() {\n    return {\n      menuItems: [],\n      businessInfo: {}\n    };\n  },\n  mounted() {\n    this.loadMenu();\n    this.loadBusinessInfo();\n  },\n  computed: {\n    total() {\n      return this.menuItems.reduce((acc, item) => acc + item.price * item.quantity, 0);\n    }\n  },\n  methods: {\n    loadMenu() {\n      const menuId = this.$route.params.menuId;\n      const menuData = getMenu(menuId);\n      if (menuData) {\n        this.menuItems = menuData.map(item => ({\n          ...item,\n          quantity: 0,\n          id: Math.random()\n        }));\n      } else {\n        alert('Menu not found.');\n        this.$router.push('/');\n      }\n    },\n    loadBusinessInfo() {\n      this.businessInfo = JSON.parse(localStorage.getItem('businessInfo')) || {};\n    },\n    increaseQuantity(item) {\n      item.quantity++;\n    },\n    decreaseQuantity(item) {\n      if (item.quantity > 0) {\n        item.quantity--;\n      }\n    },\n    sendOrder() {\n      let message = `Order from ${this.businessInfo.name}:\\n`;\n      this.menuItems.forEach(item => {\n        if (item.quantity > 0) {\n          message += `${item.name} x ${item.quantity} - $${item.price * item.quantity}\\n`;\n        }\n      });\n      message += `Total: $${this.total}\\n`;\n      message += `Contact: ${this.businessInfo.contact}`;\n      const whatsappUrl = `https://wa.me/?text=${encodeURIComponent(message)}`;\n      window.open(whatsappUrl, '_blank');\n    }\n  }\n};","map":{"version":3,"names":["getMenu","name","data","menuItems","businessInfo","mounted","loadMenu","loadBusinessInfo","computed","total","reduce","acc","item","price","quantity","methods","menuId","$route","params","menuData","map","id","Math","random","alert","$router","push","JSON","parse","localStorage","getItem","increaseQuantity","decreaseQuantity","sendOrder","message","forEach","contact","whatsappUrl","encodeURIComponent","window","open"],"sources":["F:\\Driver google\\VUE.JS-2\\temp\\websap\\src\\components\\SharedMenuView.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h1>{{ businessInfo.name }}</h1>\r\n    <img :src=\"businessInfo.logo\" alt=\"Business Logo\" style=\"max-width: 100px;\">\r\n    <p>{{ businessInfo.description }}</p>\r\n\r\n    <h2>Menu</h2>\r\n    <div v-for=\"item in menuItems\" :key=\"item.id\" class=\"menu-item\">\r\n      <img :src=\"item.image\" alt=\"Item Image\" style=\"max-width: 100px;\">\r\n      <h3>{{ item.name }}</h3>\r\n      <p>{{ item.description }}</p>\r\n      <p>Price: ${{ item.price }}</p>\r\n      <div>\r\n        <button @click=\"decreaseQuantity(item)\">-</button>\r\n        <span>{{ item.quantity }}</span>\r\n        <button @click=\"increaseQuantity(item)\">+</button>\r\n      </div>\r\n    </div>\r\n\r\n    <h2>Order Summary</h2>\r\n    <p>Total: ${{ total }}</p>\r\n    <button @click=\"sendOrder\">Send Order via WhatsApp</button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { getMenu } from '../services/menuService';\r\n\r\nexport default {\r\n  name: 'SharedMenuView',\r\n  data() {\r\n    return {\r\n      menuItems: [],\r\n      businessInfo: {},\r\n    };\r\n  },\r\n  mounted() {\r\n    this.loadMenu();\r\n    this.loadBusinessInfo();\r\n  },\r\n  computed: {\r\n    total() {\r\n      return this.menuItems.reduce((acc, item) => acc + (item.price * item.quantity), 0);\r\n    }\r\n  },\r\n  methods: {\r\n    loadMenu() {\r\n      const menuId = this.$route.params.menuId;\r\n      const menuData = getMenu(menuId);\r\n      if (menuData) {\r\n        this.menuItems = menuData.map(item => ({ ...item, quantity: 0, id: Math.random() }));\r\n      } else {\r\n        alert('Menu not found.');\r\n        this.$router.push('/');\r\n      }\r\n    },\r\n    loadBusinessInfo() {\r\n      this.businessInfo = JSON.parse(localStorage.getItem('businessInfo')) || {};\r\n    },\r\n    increaseQuantity(item) {\r\n      item.quantity++;\r\n    },\r\n    decreaseQuantity(item) {\r\n      if (item.quantity > 0) {\r\n        item.quantity--;\r\n      }\r\n    },\r\n    sendOrder() {\r\n      let message = `Order from ${this.businessInfo.name}:\\n`;\r\n      this.menuItems.forEach(item => {\r\n        if (item.quantity > 0) {\r\n          message += `${item.name} x ${item.quantity} - $${item.price * item.quantity}\\n`;\r\n        }\r\n      });\r\n      message += `Total: $${this.total}\\n`;\r\n      message += `Contact: ${this.businessInfo.contact}`;\r\n\r\n      const whatsappUrl = `https://wa.me/?text=${encodeURIComponent(message)}`;\r\n      window.open(whatsappUrl, '_blank');\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.menu-item {\r\n  border: 1px solid #ccc;\r\n  padding: 10px;\r\n  margin-bottom: 10px;\r\n}\r\n</style>\r\n"],"mappings":";;;;;AA0BA,SAASA,OAAM,QAAS,yBAAyB;AAEjD,eAAe;EACbC,IAAI,EAAE,gBAAgB;EACtBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,EAAE;MACbC,YAAY,EAAE,CAAC;IACjB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,QAAQ,CAAC,CAAC;IACf,IAAI,CAACC,gBAAgB,CAAC,CAAC;EACzB,CAAC;EACDC,QAAQ,EAAE;IACRC,KAAKA,CAAA,EAAG;MACN,OAAO,IAAI,CAACN,SAAS,CAACO,MAAM,CAAC,CAACC,GAAG,EAAEC,IAAI,KAAKD,GAAE,GAAKC,IAAI,CAACC,KAAI,GAAID,IAAI,CAACE,QAAS,EAAE,CAAC,CAAC;IACpF;EACF,CAAC;EACDC,OAAO,EAAE;IACPT,QAAQA,CAAA,EAAG;MACT,MAAMU,MAAK,GAAI,IAAI,CAACC,MAAM,CAACC,MAAM,CAACF,MAAM;MACxC,MAAMG,QAAO,GAAInB,OAAO,CAACgB,MAAM,CAAC;MAChC,IAAIG,QAAQ,EAAE;QACZ,IAAI,CAAChB,SAAQ,GAAIgB,QAAQ,CAACC,GAAG,CAACR,IAAG,KAAM;UAAE,GAAGA,IAAI;UAAEE,QAAQ,EAAE,CAAC;UAAEO,EAAE,EAAEC,IAAI,CAACC,MAAM,CAAC;QAAE,CAAC,CAAC,CAAC;MACtF,OAAO;QACLC,KAAK,CAAC,iBAAiB,CAAC;QACxB,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,GAAG,CAAC;MACxB;IACF,CAAC;IACDnB,gBAAgB