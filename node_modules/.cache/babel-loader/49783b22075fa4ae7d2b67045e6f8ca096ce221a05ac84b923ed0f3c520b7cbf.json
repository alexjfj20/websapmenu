{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.some.js\";\nimport { ref, onMounted } from 'vue';\nimport { useRouter } from 'vue-router';\nimport { getUsersFromDB, saveUsersToDB } from '../services/userService';\nexport default {\n  name: 'RegisterComponent',\n  setup() {\n    const registerForm = ref({\n      name: '',\n      email: '',\n      password: ''\n    });\n    const users = ref([]);\n    const router = useRouter();\n    onMounted(async () => {\n      // Cargar los usuarios desde IndexedDB\n      try {\n        const usersFromDB = await getUsersFromDB();\n        users.value = usersFromDB || [];\n      } catch (error) {\n        console.error('Error al cargar usuarios:', error);\n        users.value = [];\n      }\n    });\n    const register = async () => {\n      // Asegurarse de que users es un array antes de usar push\n      if (!Array.isArray(users.value)) {\n        users.value = [];\n      }\n\n      // Verificar si el email ya existe\n      if (users.value.some(user => user.email === registerForm.value.email)) {\n        alert('Este correo electrónico ya está registrado. Por favor, use otro.');\n        return;\n      }\n      const newUser = {\n        ...registerForm.value,\n        id: Date.now().toString()\n      };\n      users.value.push(newUser);\n\n      // Guardar usuarios en IndexedDB\n      try {\n        await saveUsersToDB(users.value);\n        alert('Registro exitoso. Por favor, inicie sesión.');\n        router.push('/login');\n      } catch (error) {\n        console.error('Error al guardar usuario:', error);\n        alert('Error al registrar usuario. Inténtelo nuevamente.');\n      }\n    };\n    return {\n      registerForm,\n      users,\n      register\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","useRouter","getUsersFromDB","saveUsersToDB","name","setup","registerForm","email","password","users","router","usersFromDB","value","error","console","register","Array","isArray","some","user","alert","newUser","id","Date","now","toString","push"],"sources":["F:\\Driver google\\VUE.JS-2\\temp\\websap\\src\\components\\RegisterComponent.vue"],"sourcesContent":["<template>\r\n  <div class=\"auth-container\">\r\n    <div class=\"auth-form-card\">\r\n      <h2 class=\"auth-title\">Crear Cuenta</h2>\r\n      <form @submit.prevent=\"register\" class=\"auth-form\">\r\n        <div class=\"form-group\">\r\n          <label for=\"register-name\">Nombre:</label>\r\n          <input \r\n            type=\"text\" \r\n            id=\"register-name\" \r\n            v-model=\"registerForm.name\" \r\n            class=\"auth-input\"\r\n            required\r\n            placeholder=\"Ingresa tu nombre completo\"\r\n          >\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <label for=\"register-email\">Email:</label>\r\n          <input \r\n            type=\"email\" \r\n            id=\"register-email\" \r\n            v-model=\"registerForm.email\"\r\n            class=\"auth-input\" \r\n            required\r\n            placeholder=\"Ingresa tu correo electrónico\"\r\n          >\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <label for=\"register-password\">Contraseña:</label>\r\n          <input \r\n            type=\"password\" \r\n            id=\"register-password\" \r\n            v-model=\"registerForm.password\" \r\n            class=\"auth-input\"\r\n            required\r\n            placeholder=\"Crea una contraseña segura\"\r\n          >\r\n        </div>\r\n        <button type=\"submit\" class=\"auth-button\">Registrarse</button>\r\n      </form>\r\n      <div class=\"auth-links\">\r\n        <p>¿Ya tienes una cuenta? <router-link to=\"/login\" class=\"auth-link\">Iniciar sesión</router-link></p>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, onMounted } from 'vue';\r\nimport { useRouter } from 'vue-router';\r\nimport { getUsersFromDB, saveUsersToDB } from '../services/userService';\r\n\r\nexport default {\r\n  name: 'RegisterComponent',\r\n  setup() {\r\n    const registerForm = ref({\r\n      name: '',\r\n      email: '',\r\n      password: ''\r\n    });\r\n    \r\n    const users = ref([]);\r\n    const router = useRouter();\r\n    \r\n    onMounted(async () => {\r\n      // Cargar los usuarios desde IndexedDB\r\n      try {\r\n        const usersFromDB = await getUsersFromDB();\r\n        users.value = usersFromDB || [];\r\n      } catch (error) {\r\n        console.error('Error al cargar usuarios:', error);\r\n        users.value = [];\r\n      }\r\n    });\r\n    \r\n    const register = async () => {\r\n      // Asegurarse de que users es un array antes de usar push\r\n      if (!Array.isArray(users.value)) {\r\n        users.value = [];\r\n      }\r\n      \r\n      // Verificar si el email ya existe\r\n      if (users.value.some(user => user.email === registerForm.value.email)) {\r\n        alert('Este correo electrónico ya está registrado. Por favor, use otro.');\r\n        return;\r\n      }\r\n      \r\n      const newUser = { ...registerForm.value, id: Date.now().toString() };\r\n      users.value.push(newUser);\r\n      \r\n      // Guardar usuarios en IndexedDB\r\n      try {\r\n        await saveUsersToDB(users.value);\r\n        alert('Registro exitoso. Por favor, inicie sesión.');\r\n        router.push('/login');\r\n      } catch (error) {\r\n        console.error('Error al guardar usuario:', error);\r\n        alert('Error al registrar usuario. Inténtelo nuevamente.');\r\n      }\r\n    };\r\n    \r\n    return {\r\n      registerForm,\r\n      users,\r\n      register\r\n    };\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.auth-container {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  min-height: 80vh;\r\n  padding: 20px;\r\n}\r\n\r\n.auth-form-card {\r\n  width: 100%;\r\n