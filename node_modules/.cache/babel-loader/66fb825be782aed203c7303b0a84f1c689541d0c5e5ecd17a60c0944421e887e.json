{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.for-each.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport \"core-js/modules/es.iterator.reduce.js\";\nimport { getMenu } from '../services/menuService';\nexport default {\n  name: 'SharedMenuView',\n  data() {\n    return {\n      menuItems: [],\n      businessInfo: {},\n      showBusinessInfo: false\n    };\n  },\n  mounted() {\n    this.loadMenu();\n    this.loadBusinessInfo();\n  },\n  computed: {\n    total() {\n      return this.menuItems.reduce((acc, item) => acc + item.price * item.quantity, 0);\n    }\n  },\n  methods: {\n    loadMenu() {\n      const menuId = this.$route.params.menuId;\n      const menuData = getMenu(menuId);\n      if (menuData) {\n        this.menuItems = menuData.map(item => ({\n          ...item,\n          quantity: 0,\n          id: Math.random()\n        }));\n      } else {\n        alert('Menu not found.');\n        this.$router.push('/');\n      }\n    },\n    loadBusinessInfo() {\n      this.businessInfo = JSON.parse(localStorage.getItem('businessInfo')) || {};\n    },\n    toggleBusinessInfo() {\n      this.showBusinessInfo = !this.showBusinessInfo;\n    },\n    increaseQuantity(item) {\n      item.quantity++;\n    },\n    decreaseQuantity(item) {\n      if (item.quantity > 0) {\n        item.quantity--;\n      }\n    },\n    sendOrder() {\n      let message = `Order from ${this.businessInfo.name}:\\n`;\n      this.menuItems.forEach(item => {\n        if (item.quantity > 0) {\n          message += `${item.name} x ${item.quantity} - $${item.price * item.quantity}\\n`;\n        }\n      });\n      message += `Total: $${this.total}\\n`;\n      message += `Contact: ${this.businessInfo.contact}`;\n      const whatsappUrl = `https://wa.me/?text=${encodeURIComponent(message)}`;\n      window.open(whatsappUrl, '_blank');\n    }\n  }\n};","map":{"version":3,"names":["getMenu","name","data","menuItems","businessInfo","showBusinessInfo","mounted","loadMenu","loadBusinessInfo","computed","total","reduce","acc","item","price","quantity","methods","menuId","$route","params","menuData","map","id","Math","random","alert","$router","push","JSON","parse","localStorage","getItem","toggleBusinessInfo","increaseQuantity","decreaseQuantity","sendOrder","message","forEach","contact","whatsappUrl","encodeURIComponent","window","open"],"sources":["F:\\Driver google\\VUE.JS-2\\temp\\websap\\src\\components\\SharedMenuView.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h1>{{ businessInfo.name }}</h1>\r\n    <img :src=\"businessInfo.logo\" alt=\"Business Logo\" style=\"max-width: 100px;\">\r\n    <p>{{ businessInfo.description }}</p>\r\n    \r\n    <!-- Botón para mostrar/ocultar información adicional del negocio -->\r\n    <button @click=\"toggleBusinessInfo\" class=\"toggle-button\">\r\n      {{ showBusinessInfo ? 'Ocultar información adicional' : 'Mostrar información adicional' }}\r\n    </button>\r\n    \r\n    <!-- Información adicional del negocio que se puede mostrar/ocultar -->\r\n    <div v-if=\"showBusinessInfo\" class=\"additional-info\">\r\n      <h3>Información Adicional del Negocio</h3>\r\n      <p v-if=\"businessInfo.address\"><strong>Dirección:</strong> {{ businessInfo.address }}</p>\r\n      <p v-if=\"businessInfo.email\"><strong>Email:</strong> {{ businessInfo.email }}</p>\r\n      <p v-if=\"businessInfo.contact\"><strong>Contacto:</strong> {{ businessInfo.contact }}</p>\r\n      <p v-if=\"businessInfo.hours\"><strong>Horario:</strong> {{ businessInfo.hours }}</p>\r\n      <p v-if=\"businessInfo.specialty\"><strong>Especialidad:</strong> {{ businessInfo.specialty }}</p>\r\n      <p v-if=\"businessInfo.delivery\"><strong>Delivery:</strong> {{ businessInfo.delivery === 'si' ? 'Disponible' : 'No disponible' }}</p>\r\n      \r\n      <div v-if=\"businessInfo.primaryColor && businessInfo.secondaryColor\" \r\n           class=\"color-sample\"\r\n           :style=\"{backgroundColor: businessInfo.primaryColor, color: businessInfo.secondaryColor}\">\r\n        Tema de colores del negocio\r\n      </div>\r\n    </div>\r\n\r\n    <h2>Menu</h2>\r\n    <div v-for=\"item in menuItems\" :key=\"item.id\" class=\"menu-item\">\r\n      <img :src=\"item.image\" alt=\"Item Image\" style=\"max-width: 100px;\">\r\n      <h3>{{ item.name }}</h3>\r\n      <p>{{ item.description }}</p>\r\n      <p>Price: ${{ item.price }}</p>\r\n      <div>\r\n        <button @click=\"decreaseQuantity(item)\">-</button>\r\n        <span>{{ item.quantity }}</span>\r\n        <button @click=\"increaseQuantity(item)\">+</button>\r\n      </div>\r\n    </div>\r\n\r\n    <h2>Order Summary</h2>\r\n    <p>Total: ${{ total }}</p>\r\n    <button @click=\"sendOrder\" class=\"order-button\">Send Order via WhatsApp</button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { getMenu } from '../services/menuService';\r\n\r\nexport default {\r\n  name: 'SharedMenuView',\r\n  data() {\r\n    return {\r\n      menuItems: [],\r\n      businessInfo: {},\r\n      showBusinessInfo: false\r\n    };\r\n  },\r\n  mounted() {\r\n    this.loadMenu();\r\n    this.loadBusinessInfo();\r\n  },\r\n  computed: {\r\n    total() {\r\n      return this.menuItems.reduce((acc, item) => acc + (item.price * item.quantity), 0);\r\n    }\r\n  },\r\n  methods: {\r\n    loadMenu() {\r\n      const menuId = this.$route.params.menuId;\r\n      const menuData = getMenu(menuId);\r\n      if (menuData) {\r\n        this.menuItems = menuData.map(item => ({ ...item, quantity: 0, id: Math.random() }));\r\n      } else {\r\n        alert('Menu not found.');\r\n        this.$router.push('/');\r\n      }\r\n    },\r\n    loadBusinessInfo() {\r\n      this.businessInfo = JSON.parse(localStorage.getItem('businessInfo')) || {};\r\n    },\r\n    toggleBusinessInfo() {\r\n      this.showBusinessInfo = !this.showBusinessInfo;\r\n    },\r\n    increaseQuantity(item) {\r\n      item.quantity++;\r\n    },\r\n    decreaseQuantity(item) {\r\n      if (item.quantity > 0) {\r\n        item.quantity--;\r\n      }\r\n    },\r\n    sendOrder() {\r\n      let message = `Order from ${this.businessInfo.name}:\\n`;\r\n      this.menuItems.forEach(item => {\r\n        if (item.quantity > 0) {\r\n          message += `${item.name} x ${item.quantity} - $${item.price * item.quantity}\\n`;\r\n        }\r\n      });\r\n      message += `Total: $${this.total}\\n`;\r\n      message += `Contact: ${this.businessInfo.contact}`;\r\n\r\n      const whatsappUrl = `https://wa.me/?text=${encodeURIComponent(message)}`;\r\n      window.open(whatsappUrl, '_blank');\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.menu-item {\r\n  border: 1px solid #ccc;\r\n  padding: 10px;\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.toggle-button {\r\n  background-color: #4CAF50;\r\n  color: white;\r\n  padding: 8px 15px;\r\n  border: none;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n  margin: 10px 0;\r\n  font-weight: bold;\r\n}\r\n\r\n.toggle-button:hover {\r\n  background-color: #45a049;\r\n}\r\n\r\n.additional-info {\r\n  background-color: #f9f9f9;\r\n  border-left: 4px solid #4CAF50;\r\n  padding: 15px;\r\n  margin: 15px 0;\r\n  border-radius: 4px;\r\n  text-align: left;\r\n}\r\n\r\n.color-sample {\r\n  padding: 15px;\r\n  margin-top: 10px;\r\n  border-radius: 4px;\r\n  text-align: center;\r\n  font-weight: bold;\r\n}\r\n\r\n.order-button {\r\n  background-color: #25D366;\r\n  color: white;\r\n  padding: 10px 15px;\r\n  border: none;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n  margin-top: 20px;\r\n  font-weight: bold;\r\n}\r\n\r\n.order-button:hover {\r\n  background-color: #128C7E;\r\n}\r\n</style>\r\n"],"mappings":";;;;;AAgDA,SAASA,OAAM,QAAS,yBAAyB;AAEjD,eAAe;EACbC,IAAI,EAAE,gBAAgB;EACtBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,EAAE;MACbC,YAAY,EAAE,CAAC,CAAC;MAChBC,gBAAgB,EAAE;IACpB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,QAAQ,CAAC,CAAC;IACf,IAAI