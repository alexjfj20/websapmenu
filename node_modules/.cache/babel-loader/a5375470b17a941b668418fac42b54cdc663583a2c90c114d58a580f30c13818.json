{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.find.js\";\nimport { useRouter } from 'vue-router';\nimport { saveMenu } from '../services/menuService';\nexport default {\n  name: 'LoginComponent',\n  data() {\n    return {\n      loginForm: {\n        email: '',\n        password: ''\n      },\n      users: []\n    };\n  },\n  setup() {\n    const router = useRouter();\n    return {\n      router\n    };\n  },\n  mounted() {\n    // Cargar los usuarios desde localStorage al inicio\n    const storedUsers = localStorage.getItem('users');\n    this.users = storedUsers ? JSON.parse(storedUsers) : [];\n  },\n  methods: {\n    login() {\n      // Asegurarse de que users es un array antes de usar find\n      if (!Array.isArray(this.users)) {\n        this.users = [];\n        alert('No users found. Please register first.');\n        this.router.push('/register');\n        return;\n      }\n      const user = this.users.find(user => user.email === this.loginForm.email && user.password === this.loginForm.password);\n      if (user) {\n        const menuId = saveMenu([]); // Generate a new menuId\n        this.router.push(`/menu/${menuId}`);\n      } else {\n        alert('Invalid credentials. Please try again.');\n      }\n    }\n  }\n};","map":{"version":3,"names":["useRouter","saveMenu","name","data","loginForm","email","password","users","setup","router","mounted","storedUsers","localStorage","getItem","JSON","parse","methods","login","Array","isArray","alert","push","user","find","menuId"],"sources":["F:\\Driver google\\VUE.JS-2\\temp\\websap\\src\\components\\LoginComponent.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h2>Login</h2>\r\n    <form @submit.prevent=\"login\">\r\n      <div>\r\n        <label for=\"login-email\">Email:</label>\r\n        <input type=\"email\" id=\"login-email\" v-model=\"loginForm.email\" required>\r\n      </div>\r\n     