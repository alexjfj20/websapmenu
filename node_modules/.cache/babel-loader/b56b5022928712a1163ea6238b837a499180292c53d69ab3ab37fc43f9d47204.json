{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref, reactive, onMounted, onUnmounted } from 'vue';\nimport { simulateWhatsAppReservation } from '../utils/whatsappSimulator';\nimport eventBus from '../utils/eventBus';\nexport default {\n  name: 'WhatsAppSimulator',\n  setup() {\n    const showSimulator = ref(false);\n    const status = ref(null);\n\n    // Inicializar con la fecha de ma√±ana\n    const tomorrow = new Date();\n    tomorrow.setDate(tomorrow.getDate() + 1);\n    const tomorrowStr = tomorrow.toISOString().split('T')[0];\n    const formData = reactive({\n      nombre: 'Cliente de Prueba WhatsApp',\n      telefono: '612345678',\n      email: 'whatsapp@example.com',\n      fecha: tomorrowStr,\n      hora: '20:00',\n      personas: 2,\n      notas: 'Reserva realizada desde WhatsApp'\n    });\n\n    // Funci√≥n para enviar la reserva\n    const sendReservation = async () => {\n      try {\n        status.value = {\n          type: 'info',\n          message: 'Enviando reserva...'\n        };\n\n        // Validar datos m√≠nimos\n        if (!formData.nombre || !formData.telefono || !formData.fecha || !formData.hora) {\n          status.value = {\n            type: 'error',\n            message: 'Faltan datos obligatorios'\n          };\n          return;\n        }\n\n        // Enviar la reserva usando el simulador\n        const result = await simulateWhatsAppReservation(formData);\n        if (result) {\n          status.value = {\n            type: 'success',\n            message: 'Reserva enviada correctamente'\n          };\n\n          // Notificar a trav√©s del eventBus para actualizar la lista de reservas\n          eventBus.emit('refresh-reservations');\n        } else {\n          status.value = {\n            type: 'error',\n            message: 'Error al enviar la reserva'\n          };\n        }\n      } catch (error) {\n        console.error('Error al enviar reserva:', error);\n        status.value = {\n          type: 'error',\n          message: `Error: ${error.message}`\n        };\n      }\n    };\n\n    // Funci√≥n para generar datos aleatorios\n    const generateRandomData = () => {\n      const nombres = ['Juan P√©rez', 'Mar√≠a Garc√≠a', 'Carlos Rodr√≠guez', 'Ana Mart√≠nez', 'Luis L√≥pez', 'Laura S√°nchez', 'Pedro Gonz√°lez', 'Sof√≠a Fern√°ndez'];\n      const telefonos = ['612345678', '623456789', '634567890', '645678901', '656789012', '667890123', '678901234', '689012345'];\n      const fechas = [];\n      const hoy = new Date();\n      for (let i = 1; i <= 7; i++) {\n        const fecha = new Date(hoy);\n        fecha.setDate(hoy.getDate() + i);\n        fechas.push(fecha.toISOString().split('T')[0]);\n      }\n      const horas = ['13:00', '13:30', '14:00', '14:30', '20:00', '20:30', '21:00', '21:30'];\n      const personas = [1, 2, 2, 2, 3, 4, 5, 6, 8];\n      const notas = ['Mesa cerca de la ventana si es posible', 'Celebraci√≥n de cumplea√±os', 'Alergias: gluten', 'Reserva realizada desde WhatsApp', ''];\n      return {\n        nombre: nombres[Math.floor(Math.random() * nombres.length)],\n        telefono: telefonos[Math.floor(Math.random() * telefonos.length)],\n        email: Math.random() > 0.5 ? `cliente${Math.floor(Math.random() * 100)}@example.com` : '',\n        fecha: fechas[Math.floor(Math.random() * fechas.length)],\n        hora: horas[Math.floor(Math.random() * horas.length)],\n        personas: personas[Math.floor(Math.random() * personas.length)],\n        notas: notas[Math.floor(Math.random() * notas.length)]\n      };\n    };\n\n    // Funci√≥n para enviar una reserva con datos aleatorios\n    const sendRandomReservation = () => {\n      const randomData = generateRandomData();\n\n      // Actualizar el formulario con los datos aleatorios\n      Object.assign(formData, randomData);\n\n      // Enviar la reserva\n      sendReservation();\n    };\n\n    // Escuchar respuestas de reservas\n    onMounted(() => {\n      // Configurar el eventBus para escuchar respuestas\n      eventBus.on('whatsapp_reservation_response', data => {\n        if (data.success) {\n          status.value = {\n            type: 'success',\n            message: data.message || 'Reserva recibida correctamente'\n          };\n        } else {\n          status.value = {\n            type: 'error',\n            message: data.message || 'Error al procesar la reserva'\n          };\n        }\n      });\n\n      // Inicializar con la visibilidad seg√∫n localStorage\n      const savedVisibility = localStorage.getItem('whatsapp_simulator_visible');\n      if (savedVisibility === 'true') {\n        showSimulator.value = true;\n      }\n    });\n\n    // Guardar visibilidad al desmontar\n    onUnmounted(() => {\n      eventBus.off('whatsapp_reservation_response');\n      localStorage.setItem('whatsapp_simulator_visible', showSimulator.value);\n    });\n    return {\n      showSimulator,\n      formData,\n      status,\n      sendReservation,\n      sendRandomReservation\n    };\n  }\n};","map":{"version":3,"names":["ref","reactive","onMounted","onUnmounted","simulateWhatsAppReservation","eventBus","name","setup","showSimulator","status","tomorrow","Date","setDate","getDate","tomorrowStr","toISOString","split","formData","nombre","telefono","email","fecha","hora","personas","notas","sendReservation","value","type","message","result","emit","error","console","generateRandomData","nombres","telefonos","fechas","hoy","i","push","horas","Math","floor","random","length","sendRandomReservation","randomData","Object","assign","on","data","success","savedVisibility","localStorage","getItem","off","setItem"],"sources":["F:\\Driver google\\VUE.JS-2\\VUE-JS\\websap\\src\\components\\WhatsAppSimulator.vue"],"sourcesContent":["<template>\n  <div class=\"whatsapp-simulator\">\n    <button @click=\"showSimulator = !showSimulator\" class=\"toggle-button\">\n      <span v-if=\"showSimulator\">‚ùå Cerrar Simulador</span>\n      <span v-else>üì± Simular WhatsApp</span>\n    </button>\n    \n    <div v-if=\"showSimulator\" class=\"simulator-panel\">\n      <h3>Simulador de Reservas WhatsApp</h3>\n      \n      <div class=\"form-group\">\n        <label for=\"nombre\">Nombre:</label>\n        <input type=\"text\" id=\"nombre\" v-model=\"formData.nombre\" placeholder=\"Nombre del cliente\">\n      </div>\n      \n      <div class=\"form-group\">\n        <label for=\"telefono\">Tel√©fono:</label>\n        <input type=\"text\" id=\"telefono\" v-model=\"formData.telefono\" placeholder=\"612345678\">\n      </div>\n      \n      <div class=\"form-group\">\n        <label for=\"fecha\">Fecha:</label>\n        <input type=\"date\" id=\"fecha\" v-model=\"formData.fecha\">\n      </div>\n      \n      <div class=\"form-group\">\n        <label for=\"hora\">Hora:</label>\n        <select id=\"hora\" v-model=\"formData.hora\">\n          <option value=\"13:00\">13:00</option>\n          <option value=\"13:30\">13:30</option>\n          <option value=\"14:00\">14:00</option>\n          <option value=\"14:30\">14:30</option>\n          <option value=\"20:00\">20:00</option>\n          <option value=\"20:30\">20:30</option>\n          <option value=\"21:00\">21:00</option>\n          <option value=\"21:30\">21:30</option>\n        </select>\n      </div>\n      \n      <div class=\"form-group\">\n        <label for=\"personas\">Personas:</label>\n        <select id=\"personas\" v-model=\"formData.personas\">\n          <option :value=\"1\">1 persona</option>\n          <option :value=\"2\">2 personas</option>\n          <option :value=\"3\">3 personas</option>\n          <option :value=\"4\">4 personas</option>\n          <option :value=\"5\">5 personas</option>\n          <option :value=\"6\">6 personas</option>\n          <option :value=\"8\">8 personas</option>\n        </select>\n      </div>\n      \n      <div class=\"form-group\">\n        <label for=\"notas\">Notas:</label>\n        <textarea id=\"notas\" v-model=\"formData.notas\" placeholder=\"Notas adicionales\"></textarea>\n      </div>\n      \n      <div class=\"buttons\">\n        <button @click=\"sendReservation\" class=\"send-button\">Enviar Reserva</button>\n        <button @click=\"sendRandomReservation\" class=\"random-button\">Reserva Aleatoria</button>\n      </div>\n      \n      <div v-if=\"status\" :class=\"['status', status.type]\">\n        {{ status.message }}\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { ref, reactive, onMounted, onUnmounted } from 'vue';\nimport { simulateWhatsAppReservation } from '../utils/whatsappSimulator';\nimport eventBus from '../utils/eventBus';\n\nexport default {\n  name: 'WhatsAppSimulator',\n  setup() {\n    const showSimulator = ref(false);\n    const status = ref(null);\n    \n    // Inicializar con la fecha de ma√±ana\n    const tomorrow = new Date();\n    tomorrow.setDate(tomorrow.getDate() + 1);\n    const tomorrowStr = tomorrow.toISOString().split('T')[0];\n    \n    const formData = reactive({\n      nombre: 'Cliente de Prueba WhatsApp',\n      telefono: '612345678',\n      email: 'whatsapp@example.com',\n      fecha: tomorrowStr,\n      hora: '20:00',\n      personas: 2,\n      notas: 'Reserva realizada desde WhatsApp'\n    });\n    \n    // Funci√≥n para enviar la reserva\n    const sendReservation = async () => {\n      try {\n        status.value = { type: 'info', message: 'Enviando reserva...' };\n        \n        // Validar datos m√≠nimos\n        if (!formData.nombre || !formData.telefono || !formData.fecha || !formData.hora) {\n          status.value = { type: 'error', message: 'Faltan datos obligatorios' };\n          return;\n        }\n        \n        // Enviar la reserva usando el simulador\n        const result = await simulateWhatsAppReservation(formData);\n        \n        if (result) {\n          status.value = { type: 'success', message: 'Reserva enviada correctamente' };\n          \n          // Notificar a trav√©s del eventBus para actualizar la lista de reservas\n          eventBus.emit('refresh-reservations');\n        } else {\n          status.value = { type: 'error', message: 'Error al enviar la reserva' };\n        }\n      } catch (error) {\n        console.error('Error al enviar reserva:', error);\n        status.value = { type: 'error', message: `Error: ${error.message}` };\n      }\n    };\n    \n    // Funci√≥n para generar datos aleatorios\n    const generateRandomData = () => {\n      const nombres = [\n        'Juan P√©rez', 'Mar√≠a Garc√≠a', 'Carlos Rodr√≠guez', 'Ana Mart√≠nez', \n        'Luis L√≥pez', 'Laura S√°nchez', 'Pedro Gonz√°lez', 'Sof√≠a Fern√°ndez'\n      ];\n      \n      const telefonos = [\n        '612345678', '623456789', '634567890', '645678901', \n        '656789012', '667890123', '678901234', '689012345'\n      ];\n      \n      const fechas = [];\n      const hoy = new Date();\n      for (let i = 1; i <= 7; i++) {\n        const fecha = new Date(hoy);\n        fecha.setDate(hoy.getDate() + i);\n        fechas.push(fecha.toISOString().split('T')[0]);\n      }\n      \n      const horas = ['13:00', '13:30', '14:00', '14:30', '20:00', '20:30', '21:00', '21:30'];\n      const personas = [1, 2, 2, 2, 3, 4, 5, 6, 8];\n      const notas = [\n        'Mesa cerca de la ventana si es posible',\n        'Celebraci√≥n de cumplea√±os',\n        'Alergias: gluten',\n        'Reserva realizada desde WhatsApp',\n        ''\n      ];\n      \n      return {\n        nombre: nombres[Math.floor(Math.random() * nombres.length)],\n        telefono: telefonos[Math.floor(Math.random() * telefonos.length)],\n        email: Math.random() > 0.5 ? `cliente${Math.floor(Math.random() * 100)}@example.com` : '',\n        fecha: fechas[Math.floor(Math.random() * fechas.length)],\n        hora: horas[Math.floor(Math.random() * horas.length)],\n        personas: personas[Math.floor(Math.random() * personas.length)],\n        notas: notas[Math.floor(Math.random() * notas.length)]\n      };\n    };\n    \n    // Funci√≥n para enviar una reserva con datos aleatorios\n    const sendRandomReservation = () => {\n      const randomData = generateRandomData();\n      \n      // Actualizar el formulario con los datos aleatorios\n      Object.assign(formData, randomData);\n      \n      // Enviar la reserva\n      sendReservation();\n    };\n    \n    // Escuchar respuestas de reservas\n    onMounted(() => {\n      // Configurar el eventBus para escuchar respuestas\n      eventBus.on('whatsapp_reservation_response', (data) => {\n        if (data.success) {\n          status.value = { type: 'success', message: data.message || 'Reserva recibida correctamente' };\n        } else {\n          status.value = { type: 'error', message: data.message || 'Error al procesar la reserva' };\n        }\n      });\n      \n      // Inicializar con la visibilidad seg√∫n localStorage\n      const savedVisibility = localStorage.getItem('whatsapp_simulator_visible');\n      if (savedVisibility === 'true') {\n        showSimulator.value = true;\n      }\n    });\n    \n    // Guardar visibilidad al desmontar\n    onUnmounted(() => {\n      eventBus.off('whatsapp_reservation_response');\n      localStorage.setItem('whatsapp_simulator_visible', showSimulator.value);\n    });\n    \n    return {\n      showSimulator,\n      formData,\n      status,\n      sendReservation,\n      sendRandomReservation\n    };\n  }\n};\n</script>\n\n<style scoped>\n.whatsapp-simulator {\n  position: fixed;\n  bottom: 20px;\n  right: 20px;\n  z-index: 1000;\n}\n\n.toggle-button {\n  background-color: #25D366;\n  color: white;\n  border: none;\n  border-radius: 50px;\n  padding: 10px 15px;\n  font-size: 14px;\n  cursor: pointer;\n  box-shadow: 0 2px 5px rgba(0, 0, 0, 0.2);\n  display: flex;\n  align-items: center;\n  gap: 5px;\n}\n\n.toggle-button:hover {\n  background-color: #128C7E;\n}\n\n.simulator-panel {\n  position: absolute;\n  bottom: 50px;\n  right: 0;\n  width: 300px;\n  background-color: white;\n  border-radius: 10px;\n  box-shadow: 0 5px 15px rgba(0, 0, 0, 0.2);\n  padding: 15px;\n  max-height: 80vh;\n  overflow-y: auto;\n}\n\nh3 {\n  color: #128C7E;\n  margin-top: 0;\n  text-align: center;\n  font-size: 16px;\n}\n\n.form-group {\n  margin-bottom: 10px;\n}\n\nlabel {\n  display: block;\n  margin-bottom: 5px;\n  font-size: 14px;\n  color: #333;\n}\n\ninput, select, textarea {\n  width: 100%;\n  padding: 8px;\n  border: 1px solid #ddd;\n  border-radius: 4px;\n  font-size: 14px;\n}\n\ntextarea {\n  height: 60px;\n  resize: vertical;\n}\n\n.buttons {\n  display: flex;\n  gap: 10px;\n  margin-top: 15px;\n}\n\n.send-button, .random-button {\n  flex: 1;\n  padding: 8px;\n  border: none;\n  border-radius: 4px;\n  cursor: pointer;\n  font-size: 14px;\n}\n\n.send-button {\n  background-color: #25D366;\n  color: white;\n}\n\n.random-button {\n  background-color: #34B7F1;\n  color: white;\n}\n\n.send-button:hover {\n  background-color: #128C7E;\n}\n\n.random-button:hover {\n  background-color: #0f9bd7;\n}\n\n.status {\n  margin-top: 15px;\n  padding: 10px;\n  border-radius: 4px;\n  font-size: 14px;\n  text-align: center;\n}\n\n.status.success {\n  background-color: #d4edda;\n  color: #155724;\n}\n\n.status.error {\n  background-color: #f8d7da;\n  color: #721c24;\n}\n\n.status.info {\n  background-color: #d1ecf1;\n  color: #0c5460;\n}\n</style>\n"],"mappings":";AAsEA,SAASA,GAAG,EAAEC,QAAQ,EAAEC,SAAS,EAAEC,WAAU,QAAS,KAAK;AAC3D,SAASC,2BAA0B,QAAS,4BAA4B;AACxE,OAAOC,QAAO,MAAO,mBAAmB;AAExC,eAAe;EACbC,IAAI,EAAE,mBAAmB;EACzBC,KAAKA,CAAA,EAAG;IACN,MAAMC,aAAY,GAAIR,GAAG,CAAC,KAAK,CAAC;IAChC,MAAMS,MAAK,GAAIT,GAAG,CAAC,IAAI,CAAC;;IAExB;IACA,MAAMU,QAAO,GAAI,IAAIC,IAAI,CAAC,CAAC;IAC3BD,QAAQ,CAACE,OAAO,CAACF,QAAQ,CAACG,OAAO,CAAC,IAAI,CAAC,CAAC;IACxC,MAAMC,WAAU,GAAIJ,QAAQ,CAACK,WAAW,CAAC,CAAC,CAACC,KAAK,CAAC,GAAG,CAAC,CAAC,CAAC,CAAC;IAExD,MAAMC,QAAO,GAAIhB,QAAQ,CAAC;MACxBiB,MAAM,EAAE,4BAA4B;MACpCC,QAAQ,EAAE,WAAW;MACrBC,KAAK,EAAE,sBAAsB;MAC7BC,KAAK,EAAEP,WAAW;MAClBQ,IAAI,EAAE,OAAO;MACbC,