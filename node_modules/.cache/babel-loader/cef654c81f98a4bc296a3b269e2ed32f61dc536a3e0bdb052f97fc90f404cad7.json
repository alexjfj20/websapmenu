er } from 'vue-router';\r\nimport DialogContainer from './components/DialogContainer.vue';\r\nimport { webSocketState, initializeWebSocket, reconnect, closeWebSocket } from './services/webSocketService';\r\n\r\nexport default {\r\n  name: 'App',\r\n  components: {\r\n    DialogContainer\r\n  },\r\n  setup() {\r\n    const route = useRoute();\r\n    const router = useRouter();\r\n    const isLoggedIn = ref(localStorage.getItem('isLoggedIn') === 'true');\r\n    \r\n    // WebSocket state\r\n    const wsState = webSocketState;\r\n    \r\n    // No mostrar navegación en la página de menú compartido\r\n    const showNav = computed(() => {\r\n      return !route.path.match(/^\\/menu\\/[^\\/]+$/);\r\n    });\r\n    \r\n    // Verificar estado de autenticación en cambios de ruta\r\n    watch(() => route.path, () => {\r\n      isLoggedIn.value = localStorage.getItem('isLoggedIn') === 'true';\r\n    });\r\n    \r\n    // Función de logout\r\n    const logout = () => {\r\n      localStorage.removeItem('isLoggedIn');\r\n      isLoggedIn.value = false;\r\n      router.push('/');\r\n    };\r\n    \r\n    // Inicializar WebSocket al montar el componente\r\n    onMounted(() => {\r\n      // Solo intentar conectar si hay una URL configurada - en lugar de la URL ngrok, usamos una URL relativa\r\n      // que funcionará en el mismo servidor donde está alojada la aplicación\r\n      try {\r\n        initializeWebSocket(`${window.location.protocol === 'https:' ? 'wss' : 'ws'}://${window.location.host}/ws`, {\r\n          reconnectInterval: 2000,\r\n          reconnectAttempts: 10\r\n        });\r\n      } catch (error) {\r\n        console.error('Error al inicializar WebSocket:', error);\r\n      }\r\n    });\r\n    \r\n    // Limpiar WebSocket al desmontar el componente\r\n    onUnmounted(() => {\r\n      closeWebSocket();\r\n    });\r\n    \r\n    // Manejar reconexión manual\r\n    const handleReconnect = () => {\r\n      reconnect();\r\n    };\r\n    \r\n    return {\r\n      isLoggedIn,\r\n      showNav,\r\n      logout,\r\n      wsState,\r\n      handleReconnect\r\n    };\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n/* Estilos globales */\r\n* {\r\n  box-sizing: border-box;\r\n  margin: 0;\r\n  padding: 0;\r\n}\r\n\r\nbody {\r\n  font-family: 'Avenir', Helvetica, Arial, sans-serif;\r\n  -webkit-font-smoothing: antialiased;\r\n  -moz-osx-font-smoothing: grayscale;\r\n  color: #2c3e50;\r\n  background-color: #f5f5f5;\r\n}\r\n\r\n#app {\r\n  min-height: 100vh;\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n/* Estilos para la navegación */\r\n.main-nav {\r\n  background-color: #ffffff;\r\n  box-shadow: 0 2px 8px rgba(0,0,0,0.1);\r\n  padding: 15px 0;\r\n}\r\n\r\n.nav-container {\r\n  max-width: 1200px;\r\n  margin: 0 auto;\r\n  padding: 0 20px;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n\r\n.nav-logo {\r\n  font-size: 24px;\r\n  font-weight: bold;\r\n  color: #4CAF50;\r\n  text-decoration: none;\r\n}\r\n\r\n.nav-links {\r\n  display: flex;\r\n  gap: 20px;\r\n}\r\n\r\n.nav-links a {\r\n  color: #555;\r\n  text-decoration: none;\r\n  padding: 5px 10px;\r\n  border-radius: 4px;\r\n  transition: all 0.3s;\r\n}\r\n\r\n.nav-links a:hover {\r\n  background-color: rgba(76, 175, 80, 0.1);\r\n  color: #4CAF50;\r\n}\r\n\r\n.nav-links a.router-link-exact-active {\r\n  color: #4CAF50;\r\n  font-weight: bold;\r\n}\r\n\r\n/* Ajustes responsivos */\r\n@media (max-width: 768px) {\r\n  .nav-container {\r\n    flex-direction: column;\r\n    gap: 15px;\r\n  }\r\n  \r\n  .nav-links {\r\n    flex-wrap: wrap;\r\n    justify-content: center;\r\n  }\r\n}\r\n\r\n/* WebSocket status styles */\r\n.ws-status {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  right: 0;\r\n  padding: 6px 15px;\r\n  font-size: 12px;\r\n  color: white;\r\n  z-index: 2000;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  gap: 8px;\r\n}\r\n\r\n.ws-status.reconnecting {\r\n  background-color: #ff9800;\r\n}\r\n\r\n.ws-status.error {\r\n  background-color: #f44336;\r\n}\r\n\r\n.reconnect-btn {\r\n  background-color: white;\r\n  color: #f44336;\r\n  border: none;\r\n  border-radius: 4px;\r\n  padding: 2px 8px;\r\n  font-size: 11px;\r\n  cursor: pointer;\r\n  margin-left: 8px;\r\n}\r\n\r\n.reconnect-btn:hover {\r\n  background-color: #f9f9f9;\r\n}\r\n\r\n.loading-spinner {\r\n  display: inline-block;\r\n  width: 12px;\r\n  height: 12px;\r\n  border: 2px solid rgba(255, 255, 255, 0.3);\r\n  border-radius: 50%;\r\n  border-top-color: white;\r\n  animation: spin 1s linear infinite;\r\n}\r\n\r\n.error-icon {\r\n  font-size: 14px;\r\n}\r\n\r\n@keyframes spin {\r\n  to {\r\n    transform: rotate(360deg);\r\n  }\r\n}\r\n</style>"],"mappings":";;EACOA,EAAE,EAAC;AAAK;;EADfC,GAAA;EAGqCC,KAAK,EAAC;;;EAH3CD,GAAA;EAMmCC,KAAK,EAAC;;;EANzCD,GAAA;EAWSC,KAAK,EAAC;;;EACJA,KAAK,EAAC;AAAe;;EAEnBA,KAAK,EAAC;AAAW;;;;;uBAb5BC,mBAAA,CAgCM,OAhCNC,UAgCM,GA/BJC,mBAAA,gCAAmC,EACxBC,MAAA,CAAAC,OAAO,CAACC,YAAY,I,cAA/BL,mBAAA,CAEM,OAFNM,UAEM,EAAAC,MAAA,QAAAA,MAAA,OADJC,mBAAA,CAAqC;IAA/BT,KAAK,EAAC;EAAiB,4BAJnCU,gBAAA,CAI2C,mBACvC,E,MACgBN,MAAA,CAAAC,OAAO,CAACM,KAAK,I,cAA7BV,mBAAA,CAGM,OAHNW,UAGM,G,0BAFJH,mBAAA,CAAkC;IAA5BT,KAAK,EAAC;EAAY,GAAC,IAAE,sBAPjCU,gBAAA,CAOwC,sBAAoB,GAAAG,gBAAA,CAAGT,MAAA,CAAAC,OAAO,CAACM,KAAK,IAAG,GACzE,iBAAAF,mBAAA,CAA0E;IAAjEK,OAAK,EAAAN,MAAA,QAAAA,MAAA,UAAAO,IAAA,KAAEX,MAAA,CAAAY,eAAA,IAAAZ,MAAA,CAAAY,eAAA,IAAAD,IAAA,CAAe;IAAEf,KAAK,EAAC;KAAgB,YAAU,E,KARvEG,mBAAA,gBAWgCC,MAAA,CAAAa,OAAO,I,cAAnChB,mBAAA,CAgBM,OAhBNiB,UAgBM,GAfJT,mBAAA,CAcM,OAdNU,UAcM,GAbJC,YAAA,CAAyDC,sBAAA;IAA5CC,EAAE,EAAC,GAAG;IAACtB,KAAK,EAAC;;IAblCuB,OAAA,EAAAC,QAAA,CAa6C,MAAMhB,MAAA,QAAAA,MAAA,OAbnDE,gBAAA,CAa6C,QAAM,E;IAbnDe,CAAA;MAcQhB,mBAAA,CAWM,OAXNiB,UAWM,GAVJN,YAAA,CAAwCC,sBAAA;IAA3BC,EAAE,EAAC;EAAG;IAf7BC,OAAA,EAAAC,QAAA,CAe8B,MAAMhB,MAAA,QAAAA,MAAA,OAfpCE,gBAAA,CAe8B,QAAM,E;IAfpCe,CAAA;MAgBUL,YAAA,CAAgDC,sBAAA;IAAnCC,EAAE,EAAC;EAAQ;IAhBlCC,OAAA,EAAAC,QAAA,CAgBmC,MAAShB,MAAA,QAAAA,MAAA,OAhB5