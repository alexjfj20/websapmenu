{
  "name": "websap",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "rm -rf node_modules package-lock.json && NODE_ENV=development npm install --force && npm install @vue/cli-plugin-eslint@5.0.8 --save-dev && vue-cli-service build",
    "build:render": "node render-build.js",
    "lint": "vue-cli-service lint",
    "start": "node server.js",
    "check-install": "node check-install.js"
  },
  "dependencies": {
    "@vue/cli-plugin-babel": "~5.0.0",
    "axios": "^0.27.2",
    "chart.js": "^3.7.1",
    "core-js": "^3.8.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "file-saver": "^2.0.5",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.5.25",
    "knex": "^2.0.0",
    "pg": "^8.7.3",
    "sqlite3": "^5.0.8",
    "vue": "^3.2.0",
    "vue-chartjs": "^4.1.1",
    "vue-router": "^4.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "eslint": "^8.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-eslint": "5.0.8",
    "@vue/cli-service": "5.0.8"
  },
  "engines": {
    "node": "18.x"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "vue/setup-compiler-macros": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/vue3-recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2021,
      "parser": "@babel/eslint-parser",
      "requireConfigFile": false
    },
    "rules": {
      "no-console": "off",
      "no-debugger": "off"
    }
  }
}
